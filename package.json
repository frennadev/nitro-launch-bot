{
  "name": "nitro-launch-bot",
  "version": "1.0.0",
  "description": "A clean, modular Telegram bot for Solana token launches on Pump.fun and LetsBonk",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "telegram",
    "bot",
    "solana",
    "pump.fun",
    "letsbonk",
    "token",
    "launch",
    "crypto"
  ],
  "author": "Nitro Bot Team",
  "license": "MIT",
  "dependencies": {
    "grammy": "^1.21.1",
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/runner": "^2.0.3",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "bs58": "^5.0.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/bs58": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frennadev/nitro-launch-bot.git"
  },
  "bugs": {
    "url": "https://github.com/frennadev/nitro-launch-bot/issues"
  },
  "homepage": "https://github.com/frennadev/nitro-launch-bot#readme"
} 