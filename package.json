{
  "name": "viper-launch",
  "module": "main.ts",
  "scripts": {
    "start:bot": "bun src/bot/index.ts --target bun",
    "build": "tsc --outdir build",
    "build:bot": "bun build src/index.ts --outdir build --sourcemap=linked --target bun",
    "build:job": "bun build src/bot/index.ts --outdir build/bot --sourcemap=linked --target bun",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/bun": "^1.2.14",
    "@types/node": "^22.15.23",
