{
  "name": "nitro-launch",
  "type": "module",
  "module": "main.ts",
  "scripts": {
    "build:bot": "bun build src/index.ts --outdir build --sourcemap=linked --target bun --external ws --external rpc-websockets --external @solana/web3.js",
    "build:job": "bun build src/jobs/index.ts --outdir build/jobs --sourcemap=linked --target bun --external ws --external rpc-websockets --external @solana/web3.js",
    "build": "bun run build:bot && bun run build:job",
    "start": "LIGHTWEIGHT_MODE=true ENABLE_BACKGROUND_PRELOADING=false bun run build/index.js",
    "start:production": "NODE_ENV=production LIGHTWEIGHT_MODE=true ENABLE_BACKGROUND_PRELOADING=false bun run build/index.js",
    "start:lightweight": "LIGHTWEIGHT_MODE=true ENABLE_BACKGROUND_PRELOADING=false bun run build/index.js",
    "start:full": "LIGHTWEIGHT_MODE=false ENABLE_BACKGROUND_PRELOADING=true bun run build/index.js",
    "start:bun": "bun run build/index.js",
    "start:node": "node build/index.js",
    "demo:lightweight": "node lightweight-bot.js",
    "drain-wallets": "bun run src/blockchain/mixer/drain-wallets.ts",
    "test-token-create": "tsx test-token-create.ts",
    "test-bonk-create": "tsx test-bonk-create.ts",
    "test-migration": "tsx test-migration-systems.ts",
    "test-73-wallets": "tsx test-73-wallet-system.ts",
    "test-mixer-73": "tsx test-mixer-73-wallet.ts",
    "test-parallel-safety": "tsx test-parallel-mixer-safety.ts",
    "test-parallel-real": "tsx test-parallel-mixer-real.ts",
    "test-parallel-enabled": "tsx test-parallel-enabled.ts",
    "test-smart-retry": "tsx test-smart-balance-retry.ts",
    "test-production": "tsx test-production-ready.ts",
    "test-user-experience": "tsx test-user-experience.ts",
    "test-solanatracker": "bun run test-solanatracker.ts",
    "test-pumpfun-devbuy": "bun run test-pumpfun-create-devbuy.ts",
    "test-pumpfun-simple": "bun run test-pumpfun-simple.ts",
    "test-pumpfun-create": "bun run test-pumpfun-create-only.ts",
    "test-pumpfun-two-step": "bun run test-pumpfun-two-step.ts",
    "test-pumpfun-single-tx": "bun run test-pumpfun-single-tx.ts",
    "test-integration": "bun run test-integration.ts",
    "test-bot-status": "bun run test-bot-status.ts",
    "test-bot-integration": "bun run test-bot-integration.ts",
    "dev": "bun run src/index.ts",
    "dev:job": "bun run src/jobs/index.ts",
    "setup": "chmod +x setup-local.sh && ./setup-local.sh",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/bun": "^1.2.14",
    "@types/node": "^22.15.23",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.3",
    "typescript-eslint": "^8.32.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/menu": "^1.3.0",
    "@grammyjs/storage-free": "^2.5.1",
    "@project-serum/anchor": "^0.26.0",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@raydium-io/raydium-sdk-v2": "^0.1.139-alpha",
    "@solana/buffer-layout": "^4.0.1",
    "@solana/buffer-layout-utils": "^0.2.0",
    "@solana/codecs": "^2.1.1",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.9.0",
    "bs58": "^6.0.0",
    "bullmq": "^5.53.1",
    "canvas": "^3.1.2",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0",
    "grammy": "^1.36.3",
    "ioredis": "^5.6.1",
    "mongoose": "^8.15.1",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  }
}
